{
  "name": "dgraph",
  "version": "0.1.0",
  "bin": {
    "dgraph": "bin/dgraph.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "gettask": "dotenv cross-var aws ecs list-tasks --service-name %SERVICE% --cluster %CLUSTER% --query 'taskArns[0]' --output text",
    "exec": "dotenv cross-var aws ecs execute-command --cluster %CLUSTER% --task %TASK% --container %CONTAINER% --command '/bin/bash' --interactive"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "aws-cdk": "2.3.0",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^4.1.1",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.3.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.16"
  }
}